<h1>宿泊施設投稿</h1>
<%= form_with model: @post, url: '/posts', local:true do |f| %>
 <h4>宿泊施設</h4>
 <%= f.text_field :hotel %>
 <h4>場所</h4>
 <%= f.text_field :place %>
 <h4>画像</h4>
 <%= f.attachment_field :image %>
 <p></p>
 <h4>説明</h4>
 <%= f.text_area :explantion %>
 <h4>値段</h4>
 <%= f.text_field :price %><br>
 <%= f.submit '投稿する', class: "btn btn-success" %>
<% end %>